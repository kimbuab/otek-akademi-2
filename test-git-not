~cd  ~/Docments  // dosya değiştirme
mkdir testGit  // dosyaya gitme
cd testGit // dosyanın içine girme

ls  // dosya içerisindeki  liste varsa  onu gösterir

git init // dosyanın konumuna git


PS C:\Users\Adem\testGit> .git> cd ..
